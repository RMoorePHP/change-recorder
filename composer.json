{
    "name": "rmoore/change-recorder",
    "description": "records changes made to laravel models",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Moore",
            "email": "robert@sliske.uk"
        }
    ],
    "require": {
        "illuminate/support": "^5.3",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",        
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "illuminate/database": "^5.3",
        "illuminate/events": "^5.3",
        "illuminate/routing": "^5.3"
    },
    "autoload": {
    	"psr-4": {
    		"RMoore\\ChangeRecorder\\": "src/"
    	}
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMoore\\ChangeRecorder\\ChangeRecorderServiceProvider"
            ]
        }
    }
}
